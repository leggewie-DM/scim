#!/bin/sh
set -e

case "$1" in
    configure)
        /usr/lib/@DEB_HOST_MULTIARCH@/libgtk2.0-0/gtk-query-immodules-2.0 --update-cache
        /usr/lib/@DEB_HOST_MULTIARCH@/libgtk-3-0/gtk-query-immodules-3.0 --update-cache
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

